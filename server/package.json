{
  "name": "aqualang-server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node 'dist/server.js'",
    "migrate:dev": "npx prisma migrate dev --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "build": "rimraf dist/ && graphql-codegen --config codegen.yml && tsc -p . && copyfiles -u 1 src/**/*.graphql dist/",
    "dev": "nodemon 'src/server.ts' --ignore src/api/generated/ --exec 'graphql-codegen --config codegen.yml && ts-node' src/server.ts -e ts,graphql"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-resolvers": "^1.18.2",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.27",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.7",
    "prisma": "^2.17.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@prisma/client": "^2.17.0",
    "graphql-import-node": "0.0.4",
    "apollo-server-express": "^2.21.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "helmet": "^4.4.1"
  }
}